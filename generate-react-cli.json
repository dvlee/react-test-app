{
  "usesTypeScript": true,
  "usesCssModule": false,
  "cssPreprocessor": "css",
  "testLibrary": "None",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/component/TemplateName.tsx",
        "style": "templates/component/styles.tsx",
        "index": "templates/component/index.ts"
      },
      "path": "src/components",
      "withIndex": true,
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },

    "page": {
      "customTemplates": {
        "component": "templates/page/TemplateName.tsx"
      },
      "path": "src/pages"
    },
    "layout": {
      "customTemplates": {
        "component": "templates/component/TemplateName.tsx"
      },
      "path": "src/layouts"
    }
  }
}
